<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.carrot.dao.StringMsgDao">

	<insert id="insertMsg" parameterType="StringMsg" keyProperty="id"
		useGeneratedKeys="true">
		INSERT INTO `t_string_massage`
		(`msg`,
		`lang`,
		`type`)
		VALUES
		(#{msg},
		#{lang,},
		#{type});

	</insert>




	<select id="getMsgList" parameterType="String"
		resultType="com.carrot.model.StringMsg">
		SELECT t.id,t.msg,t.lang,t.type FROM t_string_massage t
		where t.msg like CONCAT('%',#{msg },'%')
	</select>

</mapper>